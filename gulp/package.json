{
    "name": "startupkit",
    "description": "Startup Kit",
    "demo": "https://www.assistance.bg",
    "version": "1.0.0",
    "engines": {
        "ghost": ">=2.0.0",
        "ghost-api": "v2"
    },
    "license": "MIT",
    "screenshots": {
        "desktop": "assets/screenshot-desktop.jpg",
        "mobile": "assets/screenshot-mobile.jpg"
    },
    "scripts": {
        "dev": "gulp",
        "zip": "gulp zip",
        "test": "gscan .",
        "pretest": "gulp build",
        "preship": "yarn test",
        "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then yarn version && git push --follow-tags; fi"
    },
    "author": {
        "name": "Vladimir Chavkov",
        "email": "support@assistance.bg",
        "url": "https://www.assistance.bg"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "ghost",
            "theme"
        ]
    },
    "keywords": [
        "eruope"
    ],
    "repository": {
        "type": "git",
        "url": "http://git.assistance.bg:8888/ghost/ghost-scripts.git"
    },
    "config": {
        "posts_per_page": 25,
        "image_sizes": {
            "xxs": {
                "width": 30
            },
            "xs": {
                "width": 100
            },
            "s": {
                "width": 300
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1000
            },
            "xl": {
                "width": 2000
            }
        },
        "gmaps_key": "AIzaSyDNxNrchaHYQ0rDIpJddFghtvUlQH5qy28",
        "contact_form_subscribers": "vchavkov@assistance.bg",
        "contact_form_subscribers_bak": "sales@aclass.bg, a1nadzor@abv.bg, jorolll@gmail.com, georgi@gmreklama.com",
        "debug": 1,
        "jshint": 0,
        "production": 0,
        "assets_src": "assets/src",
        "assets_dist": "assets/dist",
        "css_deps": [
            "node_modules/font-awesome/css/font-awesome.min.css",
            "assets/src/css/vendor/**/*.css"
        ],
        "css_deps_maps": [
            "assets/src/css/vendor/**/*.css.map"
        ],
        "js_deps_header": [
            "node_modules/jquery/dist/jquery.min.js"
        ],
        "js_deps_header_maps": [
        ],
        "js_deps_footer": [
            "assets/src/js/vendor/**/*.js"
        ],
        "js_deps_footer_maps": [
            "assets/src/js/vendor/**/*.js.map"
        ],
        "js_deps_opt": [
            "node_modules/maphilight/jquery.maphilight.min.js",
            "assets/src/js/opt/**/*.js"
        ],
        "js_deps_opt_maps": [
            "assets/src/js/opt/**/*.js.map"
        ],
        "fonts_deps": [
            "node_modules/font-awesome/fonts/**",
            "assets/src/fonts/**"
        ]
    },
    "devDependencies": {
        "autoprefixer": "*",
        "bootstrap": "^4.0.0",
        "browser-sync": "^2.26.3",
        "cssnano": "*",
        "del": "*",
        "font-awesome": "^4.7.0",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "*",
        "gulp-bump": "*",
        "gulp-changed": "*",
        "gulp-clean": "*",
        "gulp-clean-css": "*",
        "gulp-concat": "*",
        "gulp-copy": "^1.1.0",
        "gulp-cssnano": "*",
        "gulp-debug": "*",
        "gulp-exec": "*",
        "gulp-ext-replace": "^0.3.0",
        "gulp-header": "*",
        "gulp-html-replace": "*",
        "gulp-htmlmin": "*",
        "gulp-if": "^2.0.2",
        "gulp-imagemin": "*",
        "gulp-jshint": "*",
        "gulp-livereload": "*",
        "gulp-postcss": "*",
        "gulp-rename": "*",
        "gulp-replace": "*",
        "gulp-sass": "*",
        "gulp-sourcemaps": "*",
        "gulp-uglify": "*",
        "gulp-watch": "^5.0.0",
        "gulp-zip": "*",
        "jquery": "1.12.3",
        "jquery.appear": "^1.0.1",
        "jshint": "*",
        "magnific-popup": "^1.1.0",
        "maphilight": "^1.4.0",
        "popper.js": "^1.14.3",
        "postcss-color-function": "*",
        "postcss-custom-properties": "*",
        "postcss-easy-import": "*",
        "run-sequence": "^2.2.1"
    }
}
